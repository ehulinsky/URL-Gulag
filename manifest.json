{
  "manifest_version": 2,
  "name": "URL Gulag",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.6.0.js","content.js"]
  }
  ],
  "browser_action": {
  "default_icon": "stalinicon.png"
  },
  "permissions": [
    "storage",
	"tabs"
  ],
  "web_accessible_resources":["stalin.png"]
}

